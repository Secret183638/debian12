#wget https://cdimage.debian.org/debian-cd/current/amd64/iso-cd/debian-13.0.0-amd64-netinst.iso && mv debian-13.0.0-amd64-netinst.iso debian.iso
wget https://drive.google.com/uc?export=download&id=17eWRNZplXYmu6smtznlrATt3kS8mKJmd && mv debian-edu-12.9.0-amd64-netinst.iso
qemu-img create -f qcow2 disk.qcow2 3G
echo qemu-img create -f qcow2 disk.qcow2 3G >> start
echo qemu-system-x86_64 -enable-kvm -m 2048 -cdrom debian.iso -net nic,model=virtio -net user -boot d -hda disk.qcow2 >> start
chmod +x *
qemu-system-x86_64 -enable-kvm -m 2048 -cdrom debian.iso -boot d -net nic,model=virtio -net user -hda disk.qcow2
